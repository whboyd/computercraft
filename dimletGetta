interfaceSide = "left"
targetSide = "EAST"

i = peripheral.wrap(interfaceSide)

items = i.getAvailableItems()

for k, item in pairs(items) do
  --print("dimlet " .. item.fingerprint.dmg .. " amount " .. item.size)
  if item.fingerprint.dmg == 268 then
    extra_amount = item.size - 10
    if extra_amount > 0 then
      print("exportan!")
      i.exportItem(item.fingerprint, targetSide, extra_amount)
    end
  end
end
